{
    "manifest_version": 3,
    "name": "Kovacs - Altered Carbon",
    "version": "0.0.1",
    "description": "Alters Carbon app adding a side-card to include a title and description.",
    "icons": {
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
      },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "css": [
                "styles/kovacs.css"
            ],
            "matches": [
                "https://carbon.now.sh/*"
            ]
        }
    ]
}